<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
         <Root level="Trace"><AppenderRef ref="Console"/> </Root>
             <logger level="error" name="org.apache.ibatis.io"></logger>
             <logger level="error" name="org.apache.ibatis.logging"> </logger>

       <!-- <Logger name="com.chixing" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="org.apache.ibatis.io" level="error"></Logger>
        <Root level="debug" >
            <AppenderRef ref="Console"/>
        </Root>-->

    </Loggers>


</Configuration>